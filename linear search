#include <stdio.h>

int main() {
    // Write C code here
    int a[10],n,i,x,c=0;
    c++;
    printf(" enter the size of n\n");
    scanf("%d",&n);
    c++;
    printf(" enter the elements of the array");
     for(i=0;i<n;i++)
         { 
           scanf("%d",&a[i]);
           c++;
         }
         c++;
         printf(" enter the elements to be searched");
         scanf("%d",&x);
         c++;
          for(i=0;i<n;i++)
           {
                   if(a[i]==x)
                     {
                          printf(" elements %d is found at %d",x);
                          c++;
                          break;
                     }
                     else if(i>=n)
                     {
                         printf("not found");
                     }
                     c++;
   printf ("the space complexity is %d\n",16+4*n);
  printf ("the time complexity is %d\n",c);
  return 0;
}
}
